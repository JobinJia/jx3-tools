{
  "name": "vite-vue3-template",
  "type": "module",
  "version": "0.1.2",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "description": "A vite vue3 typescript template",
  "author": "jobinJia",
  "license": "MIT",
  "keywords": [
    "vite",
    "vue3",
    "typescript"
  ],
  "engines": {
    "node": ">= 18.7.1"
  },
  "scripts": {
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "release": "bumpp --config .bumpp.config.cjs"
  },
  "dependencies": {
    "@tauri-apps/plugin-dialog": "~2.4.0",
    "alova": "^3.3.4",
    "lodash-es": "^4.17.21",
    "pinia": "^3.0.3",
    "shuimo-ui": "npm:shuimo-ui-nightly@0.3.0-alpha.14-nightly.20250606",
    "the-new-css-reset": "^1.11.3",
    "vue": "^3.5.21",
    "vue-router": "^4.5.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^5.4.1",
    "@commitlint/cli": "^19.8.1",
    "@commitlint/config-conventional": "^19.8.1",
    "@commitlint/types": "^19.8.1",
    "@iconify-json/flat-color-icons": "^1.2.3",
    "@iconify-json/fluent-emoji-flat": "^1.2.5",
    "@iconify-json/ic": "^1.2.4",
    "@iconify-json/material-symbols": "^1.2.37",
    "@iconify-json/noto": "^1.2.7",
    "@iconify-json/noto-v1": "^1.2.5",
    "@rushstack/eslint-patch": "^1.12.0",
    "@tauri-apps/api": "^2.8.0",
    "@tauri-apps/cli": "^2.8.4",
    "@tsconfig/node18": "^18.2.4",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.5.2",
    "@unocss/eslint-config": "^66.5.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitejs/plugin-vue-jsx": "^5.1.1",
    "@vue/compiler-sfc": "^3.5.21",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^13.9.0",
    "bumpp": "^10.2.3",
    "cypress": "^15.2.0",
    "eslint": "^9.36.0",
    "eslint-plugin-cypress": "^5.1.1",
    "eslint-plugin-format": "^1.0.2",
    "eslint-plugin-vue": "^10.5.0",
    "husky": "^9.1.7",
    "jsdom": "^27.0.0",
    "lint-staged": "^16.2.0",
    "naive-ui": "^2.43.1",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.1.2",
    "typescript": "~5.9.2",
    "unocss": "^66.5.1",
    "unplugin-icons": "^22.3.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vitest": "^3.2.4",
    "vue-tsc": "^3.0.7"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}

{
  "name": "jx3-tools",
  "type": "module",
  "version": "2.0.0-beta.8",
  "private": true,
  "packageManager": "pnpm@10.17.0",
  "description": "JX3 Tools - Desktop utilities for JX3 game",
  "author": "jobinJia",
  "license": "MIT",
  "keywords": [
    "vite",
    "vue3",
    "typescript"
  ],
  "engines": {
    "node": ">= 18.7.1"
  },
  "scripts": {
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "dev": "vite",
    "build": "run-p type-check build-only",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:e2e": "start-server-and-test preview http://localhost:4173 'cypress run --e2e'",
    "test:e2e:dev": "start-server-and-test 'vite dev --port 4173' http://localhost:4173 'cypress open --e2e'",
    "build-only": "vite build",
    "type-check": "vue-tsc --noEmit -p tsconfig.vitest.json --composite false",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix --ignore-path .gitignore",
    "lint:fix": "eslint . --fix",
    "prepare": "husky",
    "release": "bumpp --config .bumpp.config.cjs"
  },
  "dependencies": {
    "@tauri-apps/plugin-dialog": "~2.4.2",
    "alova": "^3.4.1",
    "lodash-es": "^4.17.22",
    "pinia": "^3.0.4",
    "shuimo-ui": "npm:shuimo-ui-nightly@0.3.0-alpha.14-nightly.20250606",
    "the-new-css-reset": "^1.11.3",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^6.7.3",
    "@commitlint/cli": "^20.2.0",
    "@commitlint/config-conventional": "^20.2.0",
    "@commitlint/types": "^20.2.0",
    "@iconify-json/flat-color-icons": "^1.2.3",
    "@iconify-json/fluent-emoji-flat": "^1.2.5",
    "@iconify-json/ic": "^1.2.4",
    "@iconify-json/material-symbols": "^1.2.50",
    "@iconify-json/noto": "^1.2.7",
    "@iconify-json/noto-v1": "^1.2.5",
    "@rushstack/eslint-patch": "^1.15.0",
    "@tauri-apps/api": "^2.9.1",
    "@tauri-apps/cli": "^2.9.6",
    "@tsconfig/node18": "^18.2.6",
    "@types/jsdom": "^27.0.0",
    "@types/node": "^25.0.3",
    "@unocss/eslint-config": "^66.5.10",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vitejs/plugin-vue-jsx": "^5.1.2",
    "@vue/compiler-sfc": "^3.5.26",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "@vueuse/core": "^14.1.0",
    "bumpp": "^10.3.2",
    "cypress": "^15.8.1",
    "eslint": "^9.39.2",
    "eslint-plugin-cypress": "^5.2.0",
    "eslint-plugin-format": "^1.1.0",
    "eslint-plugin-vue": "^10.6.2",
    "husky": "^9.1.7",
    "jsdom": "^27.3.0",
    "lint-staged": "^16.2.7",
    "naive-ui": "^2.43.2",
    "npm-run-all": "^4.1.5",
    "start-server-and-test": "^2.1.3",
    "typescript": "~5.9.3",
    "unocss": "^66.5.10",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^30.0.0",
    "vite": "^7.3.0",
    "vitest": "^4.0.16",
    "vue-tsc": "^3.2.1"
  },
  "lint-staged": {
    "*": "eslint --fix"
  }
}
